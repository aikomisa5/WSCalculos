<?xml version="1.0" encoding="UTF-8"?>
<web-app>
  <display-name>WSCalculos</display-name>

  <!-- Utilizar cuando se utiliza el context parameters del tomcat
  <listener>
    <listener-class>
            banco.config.ContextParameters
        </listener-class>
  </listener>
  <listener>
    <listener-class>
            banco.wsconsultas.jmx.JXMContextListener
        </listener-class>
  </listener>
  -->
  <!-- Listener para el uso de JAX-WS -->
  <listener>
        <listener-class>com.sun.xml.ws.transport.http.servlet.WSServletContextListener</listener-class>
  </listener>
  <servlet>
        <servlet-name>WSCalculos</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>100</load-on-startup>
  </servlet>
  <servlet-mapping>
        <servlet-name>WSCalculos</servlet-name>
        <url-pattern>/WSCalculos</url-pattern>
  </servlet-mapping>
</web-app>